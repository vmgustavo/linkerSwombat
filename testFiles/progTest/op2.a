_op2:

    loadi A0 _a
    loadi A1 _b
    subtract A0 A1
    jmpn A0 _aMEb
    ; bMEa
    loadi A0 _b
    loadi A1 _c
    subtract A0 A1
    jmpn A0 _printB2    ; bMEc
    ; cMEb
    jump _printC2

_aMEb:
    loadi A0 _a
    loadi A1 _c
    subtract A0 A1
    jmpn A0 _printA2    ; aMEc
    ; cMEa
    jump _printC2

_printA2:
    loadi A0 _a
    storei A0 IO
    jump _endOp2

_printB2:
    loadi A0 _b
    storei A0 IO
    jump _endOp2

_printC2:
    loadi A0 _c
    storei A0 IO
    jump _endOp2

_endOp2:

return
