.extern _op1
.extern _op2
.extern _op3
.extern _op4
.extern _op5

    loadi A0 IO ; get OP
    loadi A1 IO ; get A
    loadi A2 IO ; get B
    loadi A3 IO ; get C

    storei A0 _op

	loadc A6 1
    move A7 A0
    subtract A6 A7
    jmpz A6 _call_op1

	loadc A6 1
	move A7 A0
    subtract A6 A7
    jmpz A6 _call_op2

	loadc A6 3
	move A7 A0
    subtract A6 A7
    jmpz A6 _call_op3

	loadc A6 4
	move A7 A0
    subtract A6 A7
    jmpz A6 _call_op4

	loadc A6 5
	move A7 A0
    subtract A6 A7
    jmpz A6 _call_op5

    jump _end

_call_op1:
    call _op1
    jump _end
_call_op2:
    call _op2
    jump _end
_call_op3:
    call _op3
    jump _end
_call_op4:
    call _op4
    jump _end
_call_op5:
    call _op5
    jump _end

_end:
    exit
